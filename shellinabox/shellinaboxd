# vlmcsd xinetd config
# /share/CACHEDEV1_DATA/.qpkg/Optware/etc/xinetd.d/shellinaboxd
service shellinaboxd
{
        socket_type = stream
        type = UNLISTED		# Allow a TCP port that is not listed in /etc/services
        port = 4200
        protocol = tcp
        wait = no
        user = admin
		  group = administrators		  
        server = /share/CACHEDEV1_DATA/.qpkg/shellinabox/bin/shellinaboxd
        server_args = -d -u guest -g guest --background=/tmp/shellinaboxd.pid --localhost-only --disable-ssl -f favicon.ico:/share/CACHEDEV1_DATA/.qpkg/shellinabox/.qpkg_icon.ico -s /cnx_user:guest:guest:/tmp:/share/CACHEDEV1_DATA/.qpkg/shellinabox/cnx_user.sh --user-css WhiteOnBlack:+/share/CACHEDEV1_DATA/.qpkg/shellinabox/share/doc/shellinabox/white-on-black.css,\Color:-/share/CACHEDEV1_DATA/.qpkg/shellinabox/share/doc/shellinabox/color.css 1>/dev/null 2>/tmp/shellinabox.log
        flags = IPv4
        disable = no
}
